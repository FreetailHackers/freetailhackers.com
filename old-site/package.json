{
  "name": "freetail-website",
  "scripts": {
    "dev": "node-sass-watcher style.scss -o style.css -c 'tail -n +8 <input> | node_modules/node-sass/bin/node-sass > <output>'",
    "serve": "http-server",
    "css": "node-sass --include-path . style.scss style.css"
  },
  "dependencies": {
    "http-server": "^0.11.1",
    "node-sass": "^4.13.1",
    "node-sass-watcher": "^0.5.1"
  }
}
